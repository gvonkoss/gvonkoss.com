---
layout: layouts/timeline.njk
title: A little timeline
---

{%- for post in collections.about | reverse -%}
	<li class="list-item">
    <div class="job">
      <h3>
      {{ post.data.position }} @ {{ post.data.title }}
      </h3>
      <span>{{ post.data.timeline }} â€” <span>
      <a href="https://{{ post.data.website }}"
        target="_blank"
        rel="noopener noreferrer">
        {{post.data.website}}
      </a>
    </div>
    <div class="stack">
      {%- for tech in post.data.stack -%}
      <span>{{ tech }}</span>
      {%- endfor -%}
    </div>
    <div class="content">
      {{ post.templateContent | safe}}
    </div>
	</li>
{%- endfor -%}